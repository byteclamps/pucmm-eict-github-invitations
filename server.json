{
    "name":"pucmm-eict-github-invitations",
    "profile":"${ENVIRONMENT}",
    "debug":"${BOX_SERVER_DEBUG}",
    "trace":false,
    "console":"${BOX_SERVER_DEBUG}",
    "web":{
        "directoryBrowsing":false,
        "accessLogEnable":true,
        "rewrites":{
            "enable":"true"
        },
        "http":{
            "port":8080,
            "enable":true
        },
        "HTTP2":{
            "enable":true
        },
        "bindings":{
            "HTTP":{
                "port":8080,
                "HTTP2Enable":true
            },
            "ssl":{
                "listen":8443,
                "certFile":"/etc/certificates/pucmm-eict-github-invitations.cer",
                "keyFile":"/etc/certificates/pucmm-eict-github-invitations.key",
                "keypass":"${SSL_KEY_PASSWORD}"
            }
        },
        "ssl":{
            "enable":true,
            "port":8443,
            "forceSSLRedirect":true,
            "HSTS":{
                "enable":true,
                "maxAge":31536000,
                "includeSubDomains":true
            }
        },
        "gzipEnable":true,
        "AJP":{
            "enable":false
        }
    },
    "app":{
        "cfengine":"lucee@6.1.0+243",
        "restMappings":"/rest/*,/api/*"
    },
    "jvm":{
        "heapSize":"1024m",
        "minHeapSize":"512m",
        "args":"-XX:+UseG1GC -XX:-CreateMinidumpOnCrash --add-opens=java.base/java.net=ALL-UNNAMED"
    },
    "runwar":{
        "logmessageonly":true
    },
    "ModCFML":{
        "enable":true,
        "sharedKey":"my-secret"
    }
}